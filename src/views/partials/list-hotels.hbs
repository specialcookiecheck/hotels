<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Hotel name</th>
      <th>City</th>
      <th>Airport</th>
    </tr>
  </thead>
  <tbody>
    {{#each hotelList.hotels}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{city}}
        </td>
        <td>
          {{airport}}
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>